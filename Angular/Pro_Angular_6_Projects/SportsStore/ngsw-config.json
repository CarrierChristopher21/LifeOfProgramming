{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/manifest.webmanifest",
          "/*.js",
          "/fontawesome-webfont*"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/font/*"
        ]
      }
	}],
	"datagroups": [
    {
      "name": "api-product",
      "urls": [ "/api/products" ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "5d"
      }
    }
  ],
  "navigationUrls": [
    "/**"
  ]
}
